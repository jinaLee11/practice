class Solution {
    public int solution(int num1, int num2) {
        int answer = 0;
        if(num1 == num2){
            answer = 1;
            System.out.println(answer);
        } else {
            answer = -1;
            System.out.println(answer);
        }
        return answer;
    }
}

class Main {
    public static void main(String[] args) {
    
    Solution sol = new Solution();
    sol.solution(2,3);
    sol.solution(11,11);
    sol.solution(7,99);
        
    }
}


// 삼항연산자를 사용해 아래처럼 코딩할 수도 있다 !!
class Solution {
    public int solution(int num1, int num2) {
        int answer = (num1 == num2)? 1 : -1;
        return answer;
    }
}
